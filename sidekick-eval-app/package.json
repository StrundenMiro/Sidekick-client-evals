{
  "name": "sidekick-eval-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "build": "next build",
    "start": "next start -p 5000 -H 0.0.0.0",
    "lint": "eslint",
    "migrate": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/migrate-to-postgres.ts",
    "db:init": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' -e \"require('./src/lib/db').initializeSchema().then(() => process.exit(0))\""
  },
  "dependencies": {
    "next": "16.1.1",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
